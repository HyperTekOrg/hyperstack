{
  "name": "hyperstack-stacks",
  "version": "0.5.4",
  "description": "Protocol stacks for Hyperstack - ready-to-use Solana data streams",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./ore": {
      "types": "./dist/ore/index.d.ts",
      "import": "./dist/ore/index.js",
      "require": "./dist/ore/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "hyperstack-react": ">=0.5.4",
    "hyperstack-typescript": ">=0.5.4"
  },
  "dependencies": {
    "zod": "^3.24.1"
  },
  "peerDependenciesMeta": {
    "hyperstack-react": {
      "optional": true
    },
    "hyperstack-typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "hyperstack-typescript": "file:../../../typescript/core",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "hyperstack",
    "solana",
    "blockchain",
    "streaming"
  ],
  "author": "HyperTek",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HyperTekOrg/hyperstack.git",
    "directory": "stacks/sdk/typescript"
  },
  "publishConfig": {
    "access": "public"
  }
}
